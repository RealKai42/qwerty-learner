{
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "css.lint.unknownAtRules": "ignore",
  "css.validate": false,
  "files.eol": "\n",
  "search.exclude": {
    "**/public/dicts/": true,
    "**/assets/CET4_T.json": true
  },
  "cSpell.words": [
    "alipay",
    "compat",
    "Dexie",
    "esae",
    "fontawesome",
    "fortawesome",
    "headlessui",
    "heroicons",
    "IELTS",
    "immer",
    "pako",
    "romaji",
    "svgr",
    "tabler",
    "tada",
    "tailwindcss",
    "trivago",
    "ukphone",
    "ungzip",
    "usphone",
    "vercel",
    "Wechat",
    "Weixin",
    "wordlist",
    "Xiao",
    "xiaohongshu"
  ],
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  }
}
